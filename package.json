{
  "name": "tokyo",
  "version": "1.0.0",
  "description": "a restaurant site",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "lint": "eslint .",
    "build:ui": "rm -rf build && cd ../frotend/ && ng build --prod && cp -r dist/build ../backend",
