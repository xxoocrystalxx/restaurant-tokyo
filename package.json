    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16"
  }
}
